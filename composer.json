{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "alaouy/youtube": "^2.1",
        "albertcht/invisible-recaptcha": "^1.8",
        "artesaos/seotools": "^0.11.1",
        "cornford/googlmapper": "^2.29",
        "ellisthedev/laravel-5-robots": "^0.1.0",
        "laravelcollective/html": "^5.5.0",
        "ramsey/laravel-oauth2-instagram": "^2.0",
        "sammyk/laravel-facebook-sdk": "^3.5",
        "spatie/laravel-analytics": "^3.3",
        "spatie/laravel-sitemap": "^3.3",
        "thujohn/twitter": "^2.2",
        "torann/geoip": "^1.0",
        "thomaswelton/laravel-gravatar": "~1.0",
        "acacha/admin-lte-template-laravel": "4.*",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
